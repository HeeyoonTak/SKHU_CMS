<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<div id="fh5co-hero" style="margin-bottom: 0px; height: 700px">

	<!-- End fh5co-arrow -->
	<div class="container">
		<div class="col-md-8 col-md-offset-2">
			<div class="fh5co-hero-wrap" style="padding-top: 180px">
				<div class="fh5co-hero-intro">

					<div class="col-md-12"
						style="background: #f3f8f9; border-radius: 15px; padding-top: 100px; padding-bottom: 30px;">
						<form action="#" method="post">
							<div class="col-md-1"></div>
							<div class="col-md-10">
								<div class="form-group">
									<div>
										<p style="color: #444444">종합정보시스템 ID/PASSWORD로 로그인 해주세요.</p>
									</div>
									<label for="name" class="sr-only">id</label> <input
										placeholder="Id" id="id" type="text"
										class="form-control input-lg">
								</div>
							</div>
							<div class="col-md-1"></div>
							<div class="col-md-10">
								<div class="form-group">
									<label for="email" class="sr-only">password</label> <input
										placeholder="Password" id="password" type="password"
										class="form-control input-lg">
								</div>
							</div>
							<div class="col-md-12">
								<div class="form-group">
									<input type="submit" class="btn btn-primary btn-lg "
										value="login">

								</div>
							</div>


						</form>

					</div>

				</div>
			</div>
		</div>
	</div>
</div>