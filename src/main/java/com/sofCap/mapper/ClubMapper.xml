<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sofCap.mapper.ClubMapper">

	<select id="findClub" resultType="ClubDto">
		SELECT club_name
		FROM club
		WHERE club_name!='동아리연합회'; <!-- 동연제외 동아리 이름 출력 -->
	</select>

	<select id="countClub" resultType="Integer">
		SELECT COUNT(id)
		FROM club
		WHERE club_name!='동아리연합회'; <!-- 동연제외 동아리 개수 출력 -->
	</select>

</mapper>